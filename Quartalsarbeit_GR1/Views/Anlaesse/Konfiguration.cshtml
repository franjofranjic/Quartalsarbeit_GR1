@using Quartalsarbeit_GR1.Models;
@model List<Config>

@{
    ViewBag.Title = "Konfiguration";
}

<!-- TODO Side Tool mit Tab von Kategorien und Disziplinen -->

<h1>Konfiguration</h1>


<p>
    @Html.ActionLink("Kategorie hinzufügen", "addKategorie")
</p>

@foreach (Config config in Model)
{
    <h2>
        @config.Kategorie.Bezeichnung
    </h2>
    <h3>
        @config.Disziplin.Bezeichnung
    </h3>


}